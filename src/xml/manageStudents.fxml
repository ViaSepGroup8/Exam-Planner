<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="gui.StudentsController" alignment="TOP_CENTER" prefHeight="387.0" prefWidth="518.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="tableView" prefHeight="454.0" prefWidth="388.0">
         <columns>
            <TableColumn fx:id="IDColumn" prefWidth="84.0" text="ID" />
            <TableColumn fx:id="firstNameColumn" prefWidth="172.0" text="First Name" />
            <TableColumn fx:id="lastNameColumn" prefWidth="245.0" text="Last Name" />
         </columns>
      </TableView>
      <HBox prefHeight="0.0" prefWidth="388.0">
         <children>
            <TextField fx:id="IDTextField" promptText="Type ID" />
            <TextField fx:id="firstNameTextField" promptText="Type Name" />
            <TextField fx:id="lastNameTextField" prefHeight="26.0" prefWidth="215.0" promptText="Type Last Name" />
         </children>
      </HBox>
      <HBox alignment="TOP_CENTER" prefHeight="170.0" prefWidth="518.0">
         <children>
            <Button mnemonicParsing="false" onAction="#addPerson" prefHeight="120.0" prefWidth="238.0" text="Add Person">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#removePerson" prefHeight="120.0" prefWidth="111.0" text="Remove" />
            <Button mnemonicParsing="false" onAction="#saveChanges" prefHeight="120.0" prefWidth="174.0" text="SAVE CHANGES" />
         </children>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
